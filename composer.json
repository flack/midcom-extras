{
    "name": "openpsa/midcom-extras",
    "type": "midcom-package",
    "description": "Addons for openpsa/midcom",
    "keywords": ["mvc", "midgard", "framework"],
    "homepage": "http://midgard-project.org/midcom/",
    "license": "LGPL",
    "authors": [
        {
            "name": "Andreas Flack",
            "email": "flack@contentcontrol-berlin.de",
            "homepage": "http://www.contentcontrol-berlin.de/"
        }
    ],
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.php.net"
        },
        {
            "type": "pear",
            "url": "http://pear.horde.org"
        }
    ],
    "require": {
        "pear-pear/Mail": "*",
        "pear-pear/Mail_mime": "*",
        "pear-pear/Net_SMTP": "*",
        "pear-pear/Auth_SASL": "*",
        "php": ">=5.3.0",
        "openpsa/midcom": "dev-master",
        "openpsa/installer": "dev-master",
        "openpsa/quickform": "dev-master"
    },
    "minimum-stability": "dev",
    "suggest": {
        "ext-midgard2": ">=10.05.5"
    },
    "autoload": {
        "classmap": ["lib"]
    }
}
